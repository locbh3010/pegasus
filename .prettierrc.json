{
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "useTabs": false,
    "tabWidth": 4,
    "endOfLine": "lf",
    "arrowParens": "always",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "quoteProps": "as-needed",
    "plugins": ["prettier-plugin-tailwindcss"]
}
